{% extends 'base.html.twig' %}
{% block body %}
	<h1>Bienvenueee</h1>
	<form action="{{ path('security_login') }}" method="post">
		<dic class="form-group">
			<input placeholder="Adresse email ....." required name="_username" type="text" class="form-control">
		</dic>
		<dic class="form-group">
			<input placeholder="Mot de pass ....." required name="_password" type="password" class="form-control">
		</dic>
		<dic class="form-group">
			<button type="submit" class="btn btn-success">Connextion!</button>
			<a href="{{ path('security_registration') }}" class="btn btn-success">Inscription!</a>

		</dic>
	</form>
{% endblock %}
